    <dimen name="toolbarBtnSize">@dimen/abc_action_bar_default_height_material</dimen>

    <color name="toolbar_btn_bg_pressed">#11ffffff</color>

    <style name="CustomizeToolbar" parent="Widget.AppCompat.Toolbar">
