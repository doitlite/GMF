    <style name="SinaGuaranteeLabel">
        <item name="android:textSize">14sp</item>
        <item name="android:text">资金安全由新浪支付提供第三方托管</item>
        <item name="android:textColor">@color/gmf_text_black</item>
        <item name="android:layout_width">wrap_content</item>
